{
  "name": "booksville",
  "version": "1.0.0",
  "description": "a simple library app that manages its processes",
  "main": "index.js",
  "scripts": {
    "start:c": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node server.js",
    "client": "node start-client.js",
    "start:dev": "nodemon server/app.js --exec babel-node server/app.js",
    "start": "babel-node app.js",
    "build": "NODE_ENV=production webpack --config ./webpack.prod.config.js --progress --colors",
    "start2": "node ./client/src/app.js",
    "test": "NODE_ENV=test mocha test/routes_test --compilers js:babel-register --timeout 70000",
    "test:db": "sequelize db:migrate --env test",
    "coverage": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text --reporter=lcovonly mocha --compilers js:babel-register test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aimeedyki/hello-Books.git"
  },
  "keywords": [
    "Library",
    "Books"
  ],
  "author": "Amarachi Akuwudike",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aimeedyki/hello-Books/issues"
  },
  "homepage": "https://github.com/aimeedyki/hello-Books#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "json-loader": "^0.5.7",
    "url-loader": "^0.5.9",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.26.0",
